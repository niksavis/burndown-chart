{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Run Tests",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/run_tests.py",
      "console": "integratedTerminal",
      "justMyCode": true,
      "python": "${workspaceFolder}\\.venv\\Scripts\\python.exe"
    },
    {
      "name": "Python: Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": true,
      "python": "${workspaceFolder}\\.venv\\Scripts\\python.exe"
    }
  ]
}
